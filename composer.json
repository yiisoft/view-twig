{
    "name": "yiisoft/yii-twig",
    "description": "Yii Framework Twig Extension",
    "keywords": [
        "yii",
        "twig",
        "renderer"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-twig/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-twig"
    },
    "authors": [
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/strings": "^3.0@dev",
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "foxy/foxy": "^1.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/yii-jquery": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yiisoft\\Yii\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Yii\\Twig\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "tests": "config/tests.php"
        }
    }
}
